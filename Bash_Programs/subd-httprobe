#!/usr/bin/env bash
#Author: ShivamRai
#Date:/6/16/2021
#Description: The following script will perform subdomain enumeration with Sublis3r and pass the results to Tomnomnomâ€™s httprobe to check for live hosts.
#Usage:<scriptname> <domain.name.com>

target=$1
sublist3r -d $target -o subdomains.txt
cat subdomains.txt | httprobe
